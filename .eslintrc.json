{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-console": 2,
    "no-debugger": 2,
    "no-unused-vars": 1,
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-var": 2,
    "no-constant-condition": 2,
    "curly": 2,
    "spaced-comment": 2,
    "eqeqeq": 1,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ]
  }
}
